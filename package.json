{
  "name": "daebit",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "cd client && npm run build",
    "client": "cd client && ng serve",
    "client:dev": "cd client && ng serve --port 4200 --proxy-config proxy.config.json",
    "lint": "cd client && npm run lint",
    "publish:client": "cd client && npm run publish",
    "publish:server": "cd server && npm run publish",
    "server": "cd server && set ASPNETCORE_ENVIRONMENT=Production && dotnet run",
    "server:dev": "cd server && set ASPNETCORE_ENVIRONMENT=Development && dotnet watch run --urls \"http://*:5000\"",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start:dev": "concurrently --kill-others-on-fail \"npm run server:dev\" \"npm run client:dev\""
  },
  "private": true,
  "dependencies": {
    "concurrently": "^3.5.1"
  },
  "devDependencies": {}
}
